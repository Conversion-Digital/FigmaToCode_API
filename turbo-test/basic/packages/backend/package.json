    "lint": "eslint \"**/*.ts*\""
  },
  "devDependencies": {
    "@figma/plugin-typings": "github:figma/plugin-typings#readonly-preview",
    "eslint": "^8.36.0",
    "eslint-config-custom": "workspace:*",
    "tsconfig": "workspace:*",
    "tsup": "^6.7.0",
    "typescript": "^5.0.2"
  }
}
