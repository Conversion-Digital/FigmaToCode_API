    "lint": "eslint \"**/*.ts*\""
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.62.0",
    "@types/react": "^18.0.31",
    "eslint": "^8.36.0",
    "eslint-config-custom": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsconfig": "workspace:*",
    "tsup": "^6.7.0",
    "typescript": "^5.0.2"
  }
}
