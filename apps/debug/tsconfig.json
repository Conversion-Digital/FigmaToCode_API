{
  "extends": "tsconfig/nextjs.json",
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "global.d.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "typeRoots": [
      "node_modules/@types",
      "node_modules/@figma"
    ],
    "plugins": [
      {
        "name": "next"
      }
    ]
  }
}
